                                <term_definitions code="at0002">
                                    <items id="text">Does the person have capacity to participate in making recommendations on this plan?</items>
                                    <items id="description">The individual's mental capacity status.</items>
                                </term_definitions>
                                <term_definitions code="at0003">
                                    <items id="text">Has capacity</items>
                                    <items id="description">The individual has sufficient mental capacity to make the decision or plan.</items>
                                </term_definitions>
                                <term_definitions code="at0004">
                                    <items id="text">Does not have capacity</items>
                                    <items id="description">The individual does not have sufficient mental capacity to make the decision or plan.</items>
                                </term_definitions>
                                <term_definitions code="at0005">
                                    <items id="text">Indeterminate</items>
                                    <items id="description">It has not been able to determinate whether the individual has sufficient mental capacity to make decision or plan.</items>
                                </term_definitions>
                                <term_definitions code="at0006">
                                    <items id="text">If no, in what way does this person lack capacity?</items>
                                    <items id="description">Narrative description of the individual's mental capacity or incapacity.</items>
                                </term_definitions>
                                <term_definitions code="at0008">
