                                            <items id="description">Narrative description of the legal directive or order that applies to the individual.</items>
                                            <items id="comment">This data element may be used to capture legacy data that is not available in a structured format.</items>
                                        </term_definitions>
                                        <term_definitions code="at0023">
                                            <items id="text">Representative</items>
                                            <items id="description">Details of a nominated representative who has authority to act and make decisions on behalf of the individual, or who has been appointed to support the individual in decision-making.</items>
                                        </term_definitions>
                                        <term_definitions code="at0024">
                                            <items id="text">Role</items>
                                            <items id="description">Description of the representative's role.</items>
                                            <items id="comment">For example, 'Welfare Attorney', 'Substitute Welfare Attorney'. or 'Welfare Guardian'.</items>
                                        </term_definitions>
